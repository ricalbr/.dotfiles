#!/usr/bin/env bash

suckdiff &&
git checkout master
for branch in $(git for-each-ref --format='%(refname)' refs/heads/ | cut -d'/' -f3); do
	if [ "$branch" != "master" ];then
		echo $branch
		git merge $branch -m $branch
	fi
done
make && sudo make clean install
# make clean && rm -f config.h && git reset --hard origin/master
